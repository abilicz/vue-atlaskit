        "popper.js": "^1.15.0",
        "prosemirror-history": "^1.1.3",
        "prosemirror-schema-list": "^1.1.2",
        "prosemirror-utils": "^0.9.6",
        "prosemirror-view": "^1.15.2",
        "tiptap": "^1.27.1",
        "tiptap-commands": "^1.14.1",
        "vue-promised": "^1.2.0"
    },
    "resolutions": {
        "prosemirror-model": "1.8.2"
    }
